{
    "name": "kullenen/mockmysqld",
    "description": "To test your mysql based application just install mysqld",
    "require": {
        "symfony/process": "v4.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "mikey179/vfsstream": "^1.6",
        "mcustiel/phiremock": "^1.11"
    },
    "authors": [
        {
            "name": "Alexandr Kullenen"
        }
    ],
	"autoload": {
		"psr-4": {
			"" : "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Test\\" : "tests/"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	}
}
